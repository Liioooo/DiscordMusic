{
  "name": "discord-music",
  "version": "0.0.0",
  "description": "Discord Music Bot App",
  "author": {
    "name": "Liiiooo",
    "email": "leo.prei1408@gmail.com"
  },
  "main": "electron/dist/main.js",
  "scripts": {
    "postinstall": "npm run postinstall:electron",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build --prod",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p electron/tsconfig.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "electron:windows": "npm run build:prod && electron-packager . DiscordMusic --overwrite --asar --platform=win32 --arch=ia32 --icon=icons/icon256.ico --prune=true --out=release --version-string.ProductName='DiscordMusic'",
    "electron:mac": "npm run build:prod && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/icon256.ico --prune=true --out=release",
    "electron:linux": "npm run build:prod && electron-packager . DiscordMusic --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/icon256.ico --prune=true --out=release"
  },
  "build": {
    "asarUnpack": [

    ]
  },
  "dependencies": {
    "discord.js": "git://github.com/Liiioooo/discord.js.git#master",
    "ffmpeg-binaries": "^4.0.0",
    "opusscript": "0.0.6",
    "ytdl-core": "^0.28.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.4",
    "@angular/cli": "7.1.4",
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/forms": "7.1.4",
    "@angular/http": "7.1.4",
    "@angular/language-service": "7.1.4",
    "@angular/platform-browser": "7.1.4",
    "@angular/platform-browser-dynamic": "7.1.4",
    "@angular/router": "7.1.4",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "bootstrap": "^4.2.1",
    "bootswatch": "^4.2.1",
    "codelyzer": "4.5.0",
    "conventional-changelog-cli": "2.0.11",
    "core-js": "2.6.1",
    "electron": "4.0.0",
    "electron-packager": "^13.0.1",
    "electron-reload": "1.3.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "ngx-electron": "^2.0.0",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.1",
    "rxjs": "6.3.3",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "wait-on": "3.2.0",
    "webdriver-manager": "12.1.0",
    "zone.js": "0.8.26"
  }
}
